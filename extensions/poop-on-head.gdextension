[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.2
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://src/target/debug/libpoop_on_head.so"
linux.release.x86_64 =   "res://src/target/release/libpoop_on_head.so"
windows.debug.x86_64 =   "res://src/target/debug/poop_on_head.dll"
windows.release.x86_64 = "res://src/target/release/poop_on_head.dll"
macos.debug =            "res://src/target/debug/libpoop_on_head.dylib"
macos.release =          "res://src/target/release/libpoop_on_head.dylib"
macos.debug.arm64 =      "res://src/target/debug/libpoop_on_head.dylib"
macos.release.arm64 =    "res://src/target/release/libpoop_on_head.dylib"